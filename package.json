    "next": "^12.1.6",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-click-away-listener": "^2.2.1",
    "react-dom": "^18.1.0",
    "react-intl": "^6.0.4",
    "react-is": "^18.1.0",
    "react-svg": "^15.0.3",
