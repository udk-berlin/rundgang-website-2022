    "@babel/plugin-proposal-decorators": "^7.13.5",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.23.0",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "d3-scale": "3.3.0",
    "express": "^4.18.1",
    "framer-motion": "^6.3.11",
    "jspdf": "^2.5.1",
    "lodash": "^4.17.21",
    "maplibre-gl": "^2.1.9",
    "mobx": "^6.6.0",
